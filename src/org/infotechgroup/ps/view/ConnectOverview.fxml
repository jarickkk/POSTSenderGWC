<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.infotechgroup.ps.view.ConnectController">
   <children>
      <Label layoutX="60.0" layoutY="55.0" text="Geoserver host" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="60.0" />
      <TextField fx:id="host" layoutX="64.0" layoutY="84.0" minWidth="200.0" prefWidth="300.0" promptText="localhost:8080" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="80.0" />
      <Label layoutX="64.0" layoutY="122.0" text="Username" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="120.0" />
      <TextField fx:id="username" layoutX="64.0" layoutY="139.0" minWidth="200.0" prefWidth="300.0" promptText="admin" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="140.0" />
      <Label layoutX="60.0" layoutY="181.0" text="Password" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="180.0" />
      <TextField fx:id="password" layoutX="60.0" layoutY="204.0" minWidth="200.0" prefWidth="300.0" promptText="geoserver" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="200.0" />
      <Button defaultButton="true" layoutX="64.0" layoutY="250.0" minHeight="25.0" minWidth="70.0" mnemonicParsing="false" onMousePressed="#pressLoad" onMouseReleased="#connect" prefWidth="100.0" text="Connect" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="250.0" />
      <Label fx:id="status" layoutX="60.0" layoutY="292.0" textFill="RED" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="300.0" />
      <Button fx:id="disconnect" disable="true" layoutX="129.0" layoutY="250.0" mnemonicParsing="false" onAction="#disconnect" prefWidth="70.0" text="Disonnect" visible="false" AnchorPane.leftAnchor="140.0" AnchorPane.topAnchor="250.0" />
      <PasswordField layoutX="65.0" layoutY="361.0" prefWidth="150.0" promptText="geoserver" visible="false" AnchorPane.leftAnchor="65.0" AnchorPane.topAnchor="361.0" />
      <ProgressIndicator fx:id="loading" layoutX="59.0" layoutY="290.0" prefHeight="40.0" prefWidth="40.0" visible="false" AnchorPane.leftAnchor="60.0" AnchorPane.topAnchor="290.0" />
      <Button fx:id="editButton" layoutX="340.0" layoutY="80.0" minWidth="80.0" mnemonicParsing="false" onAction="#edit" text="Edit" AnchorPane.leftAnchor="390.0" AnchorPane.topAnchor="80.0" />
      <Button fx:id="saveButton" layoutX="340.0" layoutY="108.0" minWidth="80.0" mnemonicParsing="false" onAction="#save" text="Save" visible="false" AnchorPane.leftAnchor="390.0" AnchorPane.topAnchor="80.0" />
      <TextArea fx:id="textArea" layoutX="434.0" layoutY="80.0" prefHeight="200.0" prefWidth="400.0" visible="false" AnchorPane.leftAnchor="490.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="80.0" />
   </children>
